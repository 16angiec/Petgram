{
  "version": 2,
  "name": "petgram-api-test",
  "alias": ["petgram-api-test"],
  "builds":[
    { "src": "*.js", "use": "@vercel/node" }
  ],
  "routes": [
    { 
      "src": "/(.*)", 
      "dest": "index.js", 
      "methods": ["POST", "GET"]
    }
  ],
  "public": true,
  "github": {
    "enabled": true
  }
}
